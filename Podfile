platform :ios, '13.1'
# Comment the next line if you don't want to use dynamic frameworks
use_frameworks!
def generalPods
  pod 'Alamofire'
  pod 'Moya'
  pod 'AlamofireImage'
end


target 'WeatherDemoApp' do
  # Pods for WeatherDemoApp
  generalPods
end

target 'WeatherDemoAppPROD' do
  # Pods for WeatherDemoAppPROD
  generalPods

end

target 'WeatherDemoAppSTG' do

  # Pods for WeatherDemoAppSTG
  generalPods

end
